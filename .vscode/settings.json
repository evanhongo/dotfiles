{
	"breadcrumbs.enabled": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontFamily": "'Monaspace Krypton', monospace, 'FantasqueSansM Nerd Font Mono', 'MesloLGS NF'",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.insertSpaces": false,
	"editor.lineNumbers": "relative",
	"editor.suggestSelection": "first",
	"editor.tabSize": 2,
	"editor.tokenColorCustomizations": {
		"comments": "#1ddd5d"
	},
	"mdb.shell": "mongosh",
	"powermode.enabled": true,
	"powermode.presets": "exploding-rift",
	"prettier.arrowParens": "avoid",
	"prettier.semi": false,
	"prettier.singleQuote": true,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,
	"python.languageServer": "Pylance",
	"terminal.external.linuxExec": "alacritty",
	"terminal.external.osxExec": "alacritty",
	"terminal.integrated.fontFamily": "'MesloLGS NF'",
	"todo-tree.general.tags": ["TODO", "FIXME", "DONE", "NOTE"],
	"todo-tree.highlights.customHighlight": {
		"DONE": {
			"background": "green",
			"icon": "issue-closed",
			"iconColour": "green",
			"rulerColour": "black"
		},
		"FIXME": {
			"background": "red",
			"icon": "beaker",
			"iconColour": "red",
			"rulerColour": "red"
		},
		"NOTE": {
			"background": "orange",
			"icon": "note",
			"iconColour": "orange",
			"rulerColour": "orange"
		},
		"TODO": {
			"background": "purple",
			"icon": "bell",
			"iconColour": "purple",
			"rulerColour": "purple"
		}
	},
	"todo-tree.highlights.defaultHighlight": {
		"background": "green",
		"foreground": "white",
		"icon": "check",
		"iconColour": "gold",
		"rulerColour": "green",
		"type": "tag"
	},
	"todo-tree.regex.regex": "(//|#|<!--).*($TAGS):",
	"todo-tree.regex.regexCaseSensitive": false,
	"vscode-pets.petColor": "orange",
	"vscode-pets.petSize": "medium",
	"window.zoomLevel": 1,
	"workbench.colorCustomizations": {
		"diffEditor.insertedTextBackground": "#216772",
		"diffEditor.insertedTextBorder": "#00000000",
		"diffEditor.removedTextBackground": "#804f27",
		"diffEditor.removedTextBorder": "#00000000",
		"editor.selectionBackground": "#1f6fbf",
		"editor.selectionHighlightBackground": "#1d66bd90",
		"terminal.background": "#002b36",
		"terminal.foreground": "#2bd378"
	},
	"workbench.colorTheme": "Shades Of Blue",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.editor.navigationScope": "editor",
	"workbench.iconTheme": "vscode-icons",
	"workbench.panel.defaultLocation": "right",
	"workbench.preferredDarkColorTheme": "Black Italic",
	"zenMode.hideLineNumbers": false
}
